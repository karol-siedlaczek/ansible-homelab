[server]
protocol = https
http_addr =
http_port = {{ port }}
domain = {{ domain }}
root_url = %(protocol)s://grafana.%(domain)s/

[database]
type = mysql
host = {{ db.address }}:{{ db.port }}
name = {{ db.name }}
user = {{ db.user }}
password = {{ db.pass }}

[users]
allow_sign_up = false
allow_org_create = false